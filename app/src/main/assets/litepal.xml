<?xml version="1.0" encoding="utf-8"?>
<litepal>
    <!--<！ - -->
    <!--定义应用程序的数据库名称。-->
    <!--默认情况下，每个数据库名称应以.db结尾。-->
    <!--如果您没有使用.db命名数据库端，-->
    <!--LitePal会自动为您添加后缀。>-->
    <dbname value="scannews" ></dbname>

    <!--定义数据库的版本。每次要-->
    <!--升级数据库时，版本标记都会有所帮助。-->
    <!--修改您在映射标记中定义的模型，只需-->
    <!--将版本值加1，即可-->
    <!--自动处理-->
    <version value="1" ></version>

    <list>
        <mapping class="com.example.eight.scannews.utils.Channels"></mapping>
    </list>
</litepal>